<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="android:actionModeBackground">@color/blue_grey_600</item>
    </style>
    <style name="AppTheme3" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->


        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="DividerStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginTop">10dp</item>

        <item name="android:background">@color/grey_600</item>
        <!-- Supports API Level 17+ -->
        <!-- <item name="android:layout_marginStart">8dp</item> -->
        <!-- Supports API Level 17+ -->
        <!-- <item name="android:layout_marginEnd">8dp</item> -->
    </style>
</resources>
